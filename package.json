{
    "name": "vue",
    "displayName": "Vue Components",
    "description": ".vue component syntax highlighting for visual studio code",
    "version": "0.3.0",
    "publisher": "seanwash",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue Component",
                    "vue"
                ],
                "extensions": [
                    ".vue"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "vue",
                "scopeName": "text.html.vue",
                "path": "./syntaxes/vue.tmLanguage"
            }
        ]
    }
}